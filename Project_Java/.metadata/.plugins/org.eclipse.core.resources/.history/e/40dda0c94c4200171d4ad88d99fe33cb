import javax.swing.JFrame;

public class test extends JFrame{
	
	public test(){
		super ("s");
		
		setTitle("Background Color for JFrame");
		setSize(400,400);
		setLocationRelativeTo(null);
		setDefaultCloseOperation(EXIT_ON_CLOSE);
		setVisible(true);
	}
	
	public static void main(String[] args) {
		
		new test();
	}
}


